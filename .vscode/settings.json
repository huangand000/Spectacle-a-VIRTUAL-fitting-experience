{
    "python.venvPath": "${workspaceFolder}/venv/",
    "python.pythonPath": "/usr/bin/python2.7",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": true,
    },
    "python.linting.pylintArgs": [
        "--disable=E1601",
    ],
}